@page "/fetchdata"
@using CSharpSharedData;

<PageTitle>Weather forecast</PageTitle>


<MudDataGrid T=WeatherForecast Items=@forecasts Groupable=@true Virtualize=@true>
	<Columns>
        <Column T="WeatherForecast" Field=@nameof(WeatherForecast.LastRendered) />
		<Column T="WeatherForecast" Field=@nameof(WeatherForecast.Date) />
		<Column T="WeatherForecast" Field=@nameof(WeatherForecast.TemperatureC) />
		<Column T="WeatherForecast" Field=@nameof(WeatherForecast.TemperatureF) />
		<Column T="WeatherForecast" Field=@nameof(WeatherForecast.Summary) />
	</Columns>
</MudDataGrid>